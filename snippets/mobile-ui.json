{
  "Button(按钮)": {
    "prefix": "atmbutton",
    "body": [
      "<Button color=\"${1|primary,success,warning,danger,default|}\">$2</Button>"
    ],
    "description": ""
  },
  "List（列表）": {
    "prefix": "atmlist",
    "body": [
      "<List>\r",
      "  <List.Item>$1</List.Item>\r",
      "  <List.Item>$2</List.Item>\r",
      "  <List.Item>$3</List.Item>\r",
      "</List>"
    ],
    "description": ""
  },
  "Space(间距)": {
    "prefix": "atmspace",
    "body": [
      "<Space>\r",
      "  <Button>$1</Button>\r",
      "  <Button>$2</Button>\r",
      "  <Button>$3</Button>\r",
      "</Space>"
    ],
    "description": ""
  },
  "Badge(徽标)": {
    "prefix": "atmbadge",
    "body": ["<Badge count=\"$1\">$2</Badge>"],
    "description": ""
  },
  "Card(卡片)": {
    "prefix": "atmcard",
    "body": ["<Card title=\"$1\">$2</Card>"],
    "description": ""
  },
  "Image(图片)": {
    "prefix": "atmimage",
    "body": ["<Image src=\"$1\" />"],
    "description": ""
  },
  "ImageViewer(图片查看器)": {
    "prefix": "atmimageViewer",
    "body": ["<ImageViewer image=\"$1\" visible=\"$2\" />"],
    "description": ""
  },
  "InfiniteScroll(无限滚动)": {
    "prefix": "atminfiniteScroll",
    "body": ["<InfiniteScroll loadMore=\"$1\" hasMore=\"$2\" />"],
    "description": ""
  },
  "NoticeBar(通告栏)": {
    "prefix": "atmnoticeBar",
    "body": ["<NoticeBar content=\"$1\" color=\"${2|alert,info,error,default|}\" />"],
    "description": ""
  },
  "PageIndicator(页码指示器)": {
    "prefix": "atmpageIndicator",
    "body": ["<PageIndicator total=\"$1\" current=\"$2\" />"],
    "description": ""
  },
  "Popover(气泡弹出框)": {
    "prefix": "atmpopover",
    "body": [
      "<Popover content=\"$1\" placement=\"$2\" >\r",
      "  <Button>$3</Button>\r",
      "</Popover>"
    ],
    "description": ""
  },
  "Steps(步骤条)": {
    "prefix": "atmsteps",
    "body": [
      "<Steps current={1}>\r",
      "  <Step title=\"标题1\" description='描述'/>\r",
      "  <Step title=\"标题2\" description='描述'/>\r",
      "  <Step title=\"标题3\" description='描述'/>\r",
      "</Steps>"
    ],
    "description": ""
  },
  "Swiper(走马灯/轮播图)": {
    "prefix": "atmswiper",
    "body": [
      "<Swiper>\r",
      "  <Swiper.Item key={$1}>\r",
      "    <div>1</div>\r",
      "  </Swiper.Item>\r",
      "  <Swiper.Item key={$2}>\r",
      "    <div>2</div>\r",
      "  </Swiper.Item>\r",
      "</Swiper>"
    ],
    "description": ""
  },
  "Tag(标签)": {
    "prefix": "atmtag",
    "body": ["<Tag color=\"$1\">$2</Tag>"],
    "description": ""
  },
  "WaterMark(水印)": {
    "prefix": "atmwaterMark",
    "body": ["<WaterMark content=\"Ant Design Mobile\">$1</WaterMark>"],
    "description": ""
  },
  "Cascader(级联选择)": {
    "prefix": "atmcascader",
    "body": ["<Cascader options={$1} visible={$2} value={$3}>$4</Cascader>"],
    "description": ""
  },
  "CascaderView(级联选择视图)": {
    "prefix": "atmcascaderView",
    "body": ["<CascaderView options={$1} value={$2}>$3</CascaderView>"],
    "description": ""
  },
  "CheckList(可勾选列表)": {
    "prefix": "atmcheckList",
    "body": [
      "<CheckList default={['B']}>\r",
      "  <CheckList.Item value='A'>A</CheckList.Item>\r",
      "  <CheckList.Item value='B'>B</CheckList.Item>\r",
      "  <CheckList.Item value='C'>C</CheckList.Item>\r",
      "</CheckList>"
    ],
    "description": ""
  },
  "Checkbox(勾选框)": {
    "prefix": "atmcheckbox",
    "body": [
      "<Checkbox.Group value={$1} onChange={$2}>\r",
      "  <Checkbox value='A'>A</Checkbox>\r",
      "  <Checkbox value='B'>B</Checkbox>\r",
      "  <Checkbox value='C'>C</Checkbox>\r",
      "</Checkbox.Group>"
    ],
    "description": ""
  },
  "Form(表单)": {
    "prefix": "atmform",
    "body": [
      "<Form onFinish={$1} footer={$2}>\r",
      "  <Form.Item name={$1} label={$2} rules={$3}>\r",
      "    <Input placeholder=''/>\r",
      "  </Form.Item>\r",
      "</Form>"
    ],
    "description": ""
  },
  "Input(输入框)": {
    "prefix": "atminput",
    "body": ["<Input placeholder=\"$1\" value={$2} />"],
    "description": ""
  },
  "Picker(选择器)": {
    "prefix": "atmpicker",
    "body": ["<Picker columns=\"$1\" visible={$2} value={$3} />"],
    "description": ""
  },
  "PickerView(选择器视图)": {
    "prefix": "atmpickerView",
    "body": ["<PickerView columns=\"$1\" />"],
    "description": ""
  },
  "Radio(单选框)": {
    "prefix": "atmradio",
    "body": [
      "<Radio.Group value=\"A\">\r",
      "  <Space direction='vertical'>\r",
      "   <Radio value=\"A\">A</Radio>\r",
      "   <Radio value=\"B\">B</Radio>\r",
      "   <Radio value=\"C\">C</Radio>\r",
      "  </Space>\r",
      "</Radio.Group>"
    ],
    "description": ""
  },
  "Rate(评分)": {
    "prefix": "atmrate",
    "body": ["<Rate val=\"$1\" />"],
    "description": ""
  },
  "Selector(选择组)": {
    "prefix": "atmselector",
    "body": ["<Selector options={$1} value={$2} />"],
    "description": ""
  },
  "Slider(滑动输入条)": {
    "prefix": "atmslider",
    "body": ["<Slider step={$1} value={$2} />"],
    "description": ""
  },
  "Stepper(步进器)": {
    "prefix": "atmstepper",
    "body": ["<Stepper value={$1} />"],
    "description": ""
  },
  "Switch(开关)": {
    "prefix": "atmswitch",
    "body": ["<Switch checked={$1} />"],
    "description": ""
  },
  "TextArea(文本域)": {
    "prefix": "atmtextArea",
    "body": ["<TextArea placeholder=\"请输入内容\" value={$1} />"],
    "description": ""
  },
  "ActionSheet(动作面板)": {
    "prefix": "atmactionSheet",
    "body": ["<ActionSheet visible={$1} actions={$2} onClose={} />"],
    "description": ""
  },
  "Dialog(对话框)": {
    "prefix": "atmdialog",
    "body": [
      "<Button block onClick={()=>{ \r",
      "  Dialog.alert({\r",
      "   content: '',\r",
      "   onConfirm: ()=>{,\r",
      "     \r",
      "   }\r",
      "  })/>\r",
    "  }}>\r",
      "  \r",
      "</Button>"
    ],
    "description": ""
  },
  "Empty(空状态)": {
    "prefix": "atmempty",
    "body": ["<Empty description='暂无数据'/>"],
    "description": ""
  },
  "ErrorBlock(异常)": {
    "prefix": "atmerrorBlock",
    "body": ["<ErrorBlock status='default'/>"],
    "description": ""
  },
  "Loading(加载中)": {
    "prefix": "atmloading",
    "body": ["<DotLoading color=\"${1|primary,white,default|}\" />"],
    "description": ""
  },
  "Mask(遮罩层)": {
    "prefix": "atmmask",
    "body": ["<Mask visible={$1} onMaskClick={()=>{$2}} />"],
    "description": ""
  },
  "Modal(弹窗)": {
    "prefix": "atmmodal",
    "body": [
      "<Modal closeOnAction visible={$1} \r",
      "  content={$2}\r",
      "   content: '',\r",
      "   onClose: ()=>{,\r",
      "     \r",
      "   }\r",
      "  })/>\r",
      " />"
    ],
    "description": ""
  },
  "ProgressBar(进度条)": {
    "prefix": "atmprogressBar",
    "body": ["<ProgressBar percent={$1} />"],
    "description": ""
  },
  "ProgressCircle(进度圈)": {
    "prefix": "atmprogressCircle",
    "body": ["<ProgressCircle percent={$1} />"],
    "description": ""
  },
  "PullToRefresh(下拉刷新)": {
    "prefix": "atmpullToRefresh",
    "body": ["<PullToRefresh onRefresh={$1}>$2</PullToRefresh>"],
    "description": ""
  },
  "Result(结果)": {
    "prefix": "atmresult",
    "body": [
      "<Result status='${1|success,info,waiting,error,default|}' title='$2' description='$3' />"
    ],
    "description": ""
  },
  "SwipeAction(滑动操作)": {
    "prefix": "atmswipeAction",
    "body": [
      "<SwipeAction rightActions={$1} />\r"
    ],
    "description": ""
  },
  "Toast(轻提示)": {
    "prefix": "atmtoast",
    "body": [
      "<Button onClick={ () => { \r",
        "Toast.show({ \r",
          "content: '' \r",
          "afterClose: ()=>{\r",
            "\r",
          "}\r",
        "}) \r",
      "}}>\r",
      "轻提示",
      "</Button>"
    ],
    "description": ""
  },
  "AutoCenter(自动居中)": {
    "prefix": "atmautoCenter",
    "body": [
      "<AutoCenter>{$1}</AutoCenter>"
    ],
    "description": ""
  },
  "CapsuleTabs(胶囊选项卡)": {
    "prefix": "atmcapsuleTabs",
    "body": [
      "<CapsuleTabs \r",
      "  <CapsuleTabs.Tab title=\"水果\" key='$1' />\r",
      "  <CapsuleTabs.Tab title=\"蔬菜\" key='$2' />\r",
      "  <CapsuleTabs.Tab title=\"动物\" key='$3' />\r",
      "</CapsuleTabs>"
    ],
    "description": ""
  },
  "Collapse(折叠面板)": {
    "prefix": "atmcollapse",
    "body": [
      "<Collapse defaultActiveKey={['1']} \r",
      "  <Collapse.Panel title=\"第一项\" key='1'>这里是第一项的内容 </Collapse.Panel>\r",
      "  <Collapse.Panel title=\"第二项\" key='2'>这里是第二项的内容 </Collapse.Panel>\r",
      "  <Collapse.Panel title=\"第三项\" key='3'>这里是第三项的内容 </Collapse.Panel>\r",
      "</Collapse>"
    ],
    "description": ""
  },
  "Divider(分割线)": {
    "prefix": "atmdivider",
    "body": ["<Divider />"],
    "description": ""
  },
  "FloatingPanel(浮动面板)": {
    "prefix": "atmfloatingPanel",
    "body": ["<FloatingPanel anchors={$1} />"],
    "description": ""
  },
  "Grid(栅格)": {
    "prefix": "atmgrid",
    "body": [
      "<Grid columns={3} gap={8} \r",
      "  <Grid.Item>A</Grid.Item>\r",
      "  <Grid.Item>B</Grid.Item>\r",
      "  <Grid.Item>C</Grid.Item>\r",
      "</Grid>"
    ],
    "description": ""
  },
  "IndexBar(序列)": {
    "prefix": "atmindexBar",
    "body": [
      "<IndexBar \r",
      "  <IndexBar.Panel index='' title='' key=''>A</IndexBar.Panel>\r",
      "</IndexBar>"
    ],
    "description": ""
  },
  "JumboTabs(复杂选项卡)": {
    "prefix": "atmjumboTabs",
    "body": [
      "<JumboTabs \r",
      "  <JumboTabs.Tab title='水果' description='描述文案' key='fruits'  />\r",
      "  <JumboTabs.Tab title='蔬菜' description='描述文案' key='vegetables' />\r",
      "  <JumboTabs.Tab title='动物' description='描述文案' key='animals'  />\r",
      "</JumboTabs>"
    ],
    "description": ""
  },
  "NavBar(导航栏)": {
    "prefix": "atmnavBar",
    "body": ["<NavBar onBack={$1}>标题</NavBar>"],
    "description": ""
  },
  "Popup(弹出层)": {
    "prefix": "atmpopup",
    "body": ["<Popup visible={$1}>Hello</Popup>"],
    "description": ""
  },
  "SafeArea(安全区)": {
    "prefix": "atmsafeArea",
    "body": ["<SafeArea position='${1|top,bottom|}' />"],
    "description": ""
  }
}
