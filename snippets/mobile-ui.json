{
  "Button(按钮)": {
    "prefix": "atmbutton",
    "body": [
      "<Button color=\"${1|primary,success,warning,danger,default|}\">$2</Button>"
    ],
    "description": ""
  },
  "List（列表）": {
    "prefix": "atmlist",
    "body": [
      "<List>\r",
      "  <List.Item>$1</List.Item>\r",
      "  <List.Item>$2</List.Item>\r",
      "  <List.Item>$3</List.Item>\r",
      "</List>"
    ],
    "description": ""
  },
  "Space(间距)": {
    "prefix": "atmspace",
    "body": [
      "<Space>\r",
      "  <Button>$1</Button>\r",
      "  <Button>$2</Button>\r",
      "  <Button>$3</Button>\r",
      "</Space>"
    ],
    "description": ""
  },
  "Badge(徽标)": {
    "prefix": "atmbadge",
    "body": ["<Badge count=\"$1\">$2</Badge>"],
    "description": ""
  },
  "Card(卡片)": {
    "prefix": "atmcard",
    "body": ["<Card title=\"$1\">$2</Card>"],
    "description": ""
  },
  "Image(图片)": {
    "prefix": "atmimage",
    "body": ["<Image src=\"$1\" />"],
    "description": ""
  },
  "ImageViewer(图片查看器)": {
    "prefix": "atmimageViewer",
    "body": ["<ImageViewer image=\"$1\" visible=\"$2\" />"],
    "description": ""
  },
  "InfiniteScroll(无限滚动)": {
    "prefix": "atminfiniteScroll",
    "body": ["<InfiniteScroll loadMore=\"$1\" hasMore=\"$2\" />"],
    "description": ""
  },
  "NoticeBar(通告栏)": {
    "prefix": "atmnoticeBar",
    "body": ["<NoticeBar content=\"$1\" color=\"${2|alert,info,error,default|}\" />"],
    "description": ""
  },
  "PageIndicator(页码指示器)": {
    "prefix": "atmpageIndicator",
    "body": ["<PageIndicator total=\"$1\" current=\"$2\" />"],
    "description": ""
  },
  "Popover(气泡弹出框)": {
    "prefix": "atmpopover",
    "body": [
      "<Popover content=\"$1\" placement=\"$2\" >\r",
      "  <Button>$3</Button>\r",
      "</Popover>"
    ],
    "description": ""
  },
  "Steps(步骤条)": {
    "prefix": "atmsteps",
    "body": [
      "<Steps current={1}>\r",
      "  <Step title=\"标题1\" description='描述'/>\r",
      "  <Step title=\"标题2\" description='描述'/>\r",
      "  <Step title=\"标题3\" description='描述'/>\r",
      "</Steps>"
    ],
    "description": ""
  },
  "Swiper(走马灯/轮播图)": {
    "prefix": "atmswiper",
    "body": [
      "<Swiper>\r",
      "  <Swiper.Item key={$1}>\r",
      "    <div>1</div>\r",
      "  </Swiper.Item>\r",
      "  <Swiper.Item key={$2}>\r",
      "    <div>2</div>\r",
      "  </Swiper.Item>\r",
      "</Swiper>"
    ],
    "description": ""
  },
  "Tag(标签)": {
    "prefix": "atmtag",
    "body": ["<Tag color=\"$1\">$2</Tag>"],
    "description": ""
  },
  "WaterMark(水印)": {
    "prefix": "atmwaterMark",
    "body": ["<WaterMark content=\"Ant Design Mobile\">$1</WaterMark>"],
    "description": ""
  },
  "Cascader(级联选择)": {
    "prefix": "atmcascader",
    "body": ["<Cascader options={$1} visible={$2} value={$3}>$4</Cascader>"],
    "description": ""
  }
}
