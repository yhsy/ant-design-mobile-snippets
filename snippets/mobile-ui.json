{
  "Button(按钮)": {
    "prefix": "atmbutton",
    "body": [
      "<Button color=\"${1|primary,success,warning,danger,default|}\">$2</Button>"
    ],
    "description": ""
  },
  "List（列表）": {
    "prefix": "atmlist",
    "body": [
      "<List>\r",
      "  <List.Item>$1</List.Item>\r",
      "  <List.Item>$2</List.Item>\r",
      "  <List.Item>$3</List.Item>\r",
      "</List>"
    ],
    "description": ""
  },
  "Space(间距)": {
    "prefix": "atmspace",
    "body": [
      "<Space>\r",
      "  <Button>$1</Button>\r",
      "  <Button>$2</Button>\r",
      "  <Button>$3</Button>\r",
      "</Space>"
    ],
    "description": ""
  },
  "Badge(徽标)": {
    "prefix": "atmbadge",
    "body": ["<Badge count=\"$1\">$2</Badge>"],
    "description": ""
  },
  "Card(卡片)": {
    "prefix": "atmcard",
    "body": ["<Card title=\"$1\">$2</Card>"],
    "description": ""
  },
  "Image(图片)": {
    "prefix": "atmimage",
    "body": ["<Image src=\"$1\" />"],
    "description": ""
  },
  "ImageViewer(图片查看器)": {
    "prefix": "atmimageViewer",
    "body": ["<ImageViewer image=\"$1\" visible=\"$2\" />"],
    "description": ""
  },
  "InfiniteScroll(无限滚动)": {
    "prefix": "atminfiniteScroll",
    "body": ["<InfiniteScroll loadMore=\"$1\" hasMore=\"$2\" />"],
    "description": ""
  },
  "NoticeBar(通告栏)": {
    "prefix": "atmnoticeBar",
    "body": ["<NoticeBar content=\"$1\" color=\"${2|alert,info,error,default|}\" />"],
    "description": ""
  },
  "PageIndicator(页码指示器)": {
    "prefix": "atmpageIndicator",
    "body": ["<PageIndicator total=\"$1\" current=\"$2\" />"],
    "description": ""
  },
  "Popover(气泡弹出框)": {
    "prefix": "atmpopover",
    "body": [
      "<Popover content=\"$1\" placement=\"$2\" >\r",
      "  <Button>$3</Button>\r",
      "</Popover>"
    ],
    "description": ""
  },
  "Steps(步骤条)": {
    "prefix": "atmsteps",
    "body": [
      "<Steps current={1}>\r",
      "  <Step title=\"标题1\" description='描述'/>\r",
      "  <Step title=\"标题2\" description='描述'/>\r",
      "  <Step title=\"标题3\" description='描述'/>\r",
      "</Steps>"
    ],
    "description": ""
  },
  "Swiper(走马灯/轮播图)": {
    "prefix": "atmswiper",
    "body": [
      "<Swiper>\r",
      "  <Swiper.Item key={$1}>\r",
      "    <div>1</div>\r",
      "  </Swiper.Item>\r",
      "  <Swiper.Item key={$2}>\r",
      "    <div>2</div>\r",
      "  </Swiper.Item>\r",
      "</Swiper>"
    ],
    "description": ""
  },
  "Tag(标签)": {
    "prefix": "atmtag",
    "body": ["<Tag color=\"$1\">$2</Tag>"],
    "description": ""
  },
  "WaterMark(水印)": {
    "prefix": "atmwaterMark",
    "body": ["<WaterMark content=\"Ant Design Mobile\">$1</WaterMark>"],
    "description": ""
  },
  "Cascader(级联选择)": {
    "prefix": "atmcascader",
    "body": ["<Cascader options={$1} visible={$2} value={$3}>$4</Cascader>"],
    "description": ""
  },
  "CascaderView(级联选择视图)": {
    "prefix": "atmcascaderView",
    "body": ["<CascaderView options={$1} value={$2}>$3</CascaderView>"],
    "description": ""
  },
  "CheckList(可勾选列表)": {
    "prefix": "atmcheckList",
    "body": [
      "<CheckList default={['B']}>\r",
      "  <CheckList.Item value='A'>A</CheckList.Item>\r",
      "  <CheckList.Item value='B'>B</CheckList.Item>\r",
      "  <CheckList.Item value='C'>C</CheckList.Item>\r",
      "</CheckList>"
    ],
    "description": ""
  },
  "Checkbox(勾选框)": {
    "prefix": "atmcheckbox",
    "body": [
      "<Checkbox.Group value={$1} onChange={$2}>\r",
      "  <Checkbox value='A'>A</Checkbox>\r",
      "  <Checkbox value='B'>B</Checkbox>\r",
      "  <Checkbox value='C'>C</Checkbox>\r",
      "</Checkbox.Group>"
    ],
    "description": ""
  },
  "Form(表单)": {
    "prefix": "atmform",
    "body": [
      "<Form onFinish={$1} footer={$2}>\r",
      "  <Form.Item name={$1} label={$2} rules={$3}>\r",
      "    <Input placeholder=''/>\r",
      "  </Form.Item>\r",
      "</Form>"
    ],
    "description": ""
  },
  "Input(输入框)": {
    "prefix": "atminput",
    "body": ["<Input placeholder=\"$1\" value={$2} />"],
    "description": ""
  },
  "Picker(选择器)": {
    "prefix": "atmpicker",
    "body": ["<Picker columns=\"$1\" visible={$2} value={$3} />"],
    "description": ""
  },
  "PickerView(选择器视图)": {
    "prefix": "atmpickerView",
    "body": ["<PickerView columns=\"$1\" />"],
    "description": ""
  },
  "Radio(单选框)": {
    "prefix": "atmradio",
    "body": [
      "<Radio.Group value=\"A\">\r",
      "  <Space direction='vertical'>\r",
      "   <Radio value=\"A\">A</Radio>\r",
      "   <Radio value=\"B\">B</Radio>\r",
      "   <Radio value=\"C\">C</Radio>\r",
      "  </Space>\r",
      "</Radio.Group>"
    ],
    "description": ""
  },
  "Rate(评分)": {
    "prefix": "atmrate",
    "body": ["<Rate val=\"$1\" />"],
    "description": ""
  },
  "Selector(选择组)": {
    "prefix": "atmselector",
    "body": ["<Selector options={$1} value={$2} />"],
    "description": ""
  },
  "Slider(滑动输入条)": {
    "prefix": "atmslider",
    "body": ["<Slider step={$1} value={$2} />"],
    "description": ""
  },
  "Stepper(步进器)": {
    "prefix": "atmstepper",
    "body": ["<Stepper value={$1} />"],
    "description": ""
  },
  "Switch(开关)": {
    "prefix": "atmswitch",
    "body": ["<Switch checked={$1} />"],
    "description": ""
  },
  "TextArea(文本域)": {
    "prefix": "atmtextArea",
    "body": ["<TextArea placeholder=\"请输入内容\" value={$1} />"],
    "description": ""
  },
  "ActionSheet(动作面板)": {
    "prefix": "atmactionSheet",
    "body": ["<ActionSheet visible={$1} actions={$2} onClose={} />"],
    "description": ""
  },
  "Dialog(对话框)": {
    "prefix": "atmdialog",
    "body": [
      "<Button block onClick={()=>{ \r",
      "  Dialog.alert({\r",
      "   content: '',\r",
      "   onConfirm: ()=>{,\r",
      "     ",
      "   }\r",
      "  })/>",
    "  }}>",
      "  \r",
      "</Button>"
    ],
    "description": ""
  },
  "Empty(空状态)": {
    "prefix": "atmempty",
    "body": ["<Empty description='暂无数据'/>"],
    "description": ""
  },
  "ErrorBlock(异常)": {
    "prefix": "atmerrorBlock",
    "body": ["<ErrorBlock status='default'/>"],
    "description": ""
  },
  "Loading(加载中)": {
    "prefix": "atmloading",
    "body": ["<DotLoading color=\"${1|primary,white,default|}\" />"],
    "description": ""
  },
  "Mask(遮罩层)": {
    "prefix": "atmmask",
    "body": ["<Mask visible={$1} onMaskClick={()=>{$2}} />"],
    "description": ""
  }
}
