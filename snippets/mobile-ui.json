{
  "Button(按钮)": {
    "prefix": "atmbutton",
    "body": [
      "<Button color=\"${1|primary,success,warning,danger,default|}\">$2</Button>"
    ],
    "description": ""
  },
  "List（列表）": {
    "prefix": "atmlist",
    "body": [
      "<List>\r",
      "  <List.Item>$1</List.Item>\r",
      "  <List.Item>$2</List.Item>\r",
      "  <List.Item>$3</List.Item>\r",
      "</List>"
    ],
    "description": ""
  },
  "Space(间距)": {
    "prefix": "atmspace",
    "body": [
      "<Space>\r",
      "  <Button>$1</Button>\r",
      "  <Button>$2</Button>\r",
      "  <Button>$3</Button>\r",
      "</Space>"
    ],
    "description": ""
  },
  "Badge(徽标)": {
    "prefix": "atmbadge",
    "body": ["<Badge count=\"$1\">$2</Badge>"],
    "description": ""
  },
  "Card(卡片)": {
    "prefix": "atmcard",
    "body": ["<Card title=\"$1\">$2</Card>"],
    "description": ""
  }
}
